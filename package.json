{
  "name": "linex",
  "version": "2.0.0",
  "description": "Refined State Management.",
  "main": "build/linex.js",
  "umd:main": "build/linex.umd.js",
  "repository": "github:naminho/linex",
  "author": "Matthias Giger",
  "license": "MIT",
  "scripts": {
    "start": "microbundle watch --no-compress --output build --jsx React.createElement --format cjs,umd",
    "build": "microbundle --output build --jsx React.createElement --format cjs,umd",
    "doc": "docy --build",
    "test": "jest --watchAll",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "es6-promise": "^4.2.6",
    "fancy-proxy": "0.1.0",
    "immer": "^3.1.1",
    "invariant": "^2.2.4",
    "is-object": "^1.0.1",
    "memoize-one": "^5.0.4",
    "object-assign": "^4.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-fast-compare": "^2.0.4",
    "remove-array-item": "^1.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.7.1",
    "babel-polyfill": "^6.26.0",
    "docy": "^1.0.3",
    "jest": "^24.7.1",
    "react": "^16.8.6",
    "microbundle": "^0.11.0",
    "node-fetch": "^2.3.0",
    "react-test-renderer": "^16.8.6"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/jest.init.js"
    ],
    "moduleNameMapper": {
      "linex": "<rootDir>/src",
      "fancy-proxy": "<rootDir>/../fancy-proxy/src"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/stores/"
    ]
  },
  "files": [
    "build"
  ],
  "keywords": [
    "redux",
    "immer",
    "immutability",
    "simple",
    "state",
    "management"
  ]
}
